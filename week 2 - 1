import java.util.Scanner;

public class Quiz1 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		int year = 0;
		year = s.nextInt();
		
		if (year % 4 == 0) {
			if (year % 100 != 0) {
				year = 1;
			} else if (year % 400 == 0) {
				year = 1;
			} else {
				year = 0;
			}
		} else {
			year = 0;
		}
		
		System.out.println(year);
		
		s.close();

	}

}
